{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "tsc": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "sls:deploy": {
      "cache": false
    },
    "sls:remove": {
      "cache": false
    },
    "sls:invoke": {
      "cache": false
    }
  }
}
